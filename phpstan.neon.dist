includes:
  - vendor/larastan/larastan/extension.neon

parameters:
  level: 5
  paths:
    - app
    - config
    - database
    - routes
  bootstrapFiles:
    - config/app.php

  ignoreErrors:
    - '#Call to an undefined method Illuminate\\Database\\Eloquent\\Model#'
    - '#Method.*does not exist on class App\\Models\\#'

  laravel:
    search_paths:
      - app
